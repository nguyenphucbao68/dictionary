{
  "name": "learn-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start:be": "cd backend && php -S localhost:8080",
    "dev": "set NODE_ENV=development && node server.js",
    "build": "next build",
    "start": "set NODE_ENV=production && node server.js",
    "lint": "eslint \"{components,pages,lib,config}/**/*.{ts,tsx,js,jsx}\" --fix --max-warnings=0",
    "format": "prettier --write \"./**/*.(js|jsx|json|ts|tsx)\" --end-of-line lf",
    "format:check": "prettier --check \"./**/*.(js|jsx|json|ts|tsx)\" --end-of-line lf"
  },
  "dependencies": {
    "@lourenci/react-kanban": "^0.14.0",
    "@zeit/next-css": "^1.0.1",
    "bootstrap-scss": "^4.5.2",
    "chartist": "^0.11.4",
    "firebase": "^8.1.1",
    "html-entities": "^1.3.1",
    "jsdom": "^16.4.0",
    "next": "9.5.3",
    "next-connect-redux": "^0.1.5",
    "next-images": "^1.6.2",
    "next-redux-wrapper": "^6.0.2",
    "next-seo": "^4.17.0",
    "react": "16.13.1",
    "react-audio-player": "^0.14.0",
    "react-bootstrap": "^1.3.0",
    "react-chartist": "^0.14.3",
    "react-data-table-component": "^6.11.5",
    "react-dom": "^17.0.1",
    "react-feather": "^2.0.8",
    "react-loading-skeleton": "^2.1.1",
    "react-redux": "^7.2.1",
    "react-scrollbar": "^0.5.6",
    "react-slick": "^0.27.12",
    "react-string-replace": "^0.4.4",
    "react-switch-lang": "^1.0.2",
    "react-toastify": "^6.0.8",
    "react-youtube": "^7.13.0",
    "reactstrap": "^8.6.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@types/faker": "^5.1.4",
    "@types/node": "^14.14.10",
    "@types/node-emoji": "^1.8.1",
    "@types/react": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "cheerio": "^1.0.0-rc.3",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-react": "^7.21.5",
    "express": "^4.17.1",
    "http-proxy-middleware": "^1.0.6",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.2",
    "prettier": "^2.2.1",
    "typescript": "^4.1.2",
    "url-loader": "^4.1.0",
    "xml2js": "^0.4.23"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix",
    "*.{ts,js,json,md}": "prettier --write",
    "../frontend/**/*.{ts,tsx,js,jsx,json,css,md}": "prettier --write"
  }
}
